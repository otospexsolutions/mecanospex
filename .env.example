# AutoERP Environment Configuration
# Copy this file to .env and customize for your environment
# NEVER commit .env to version control

# =============================================================================
# PostgreSQL (TimescaleDB)
# Using 5433 to avoid conflicts with local PostgreSQL installations
# =============================================================================
POSTGRES_PORT=5433
POSTGRES_PASSWORD=autoerp_secret

# =============================================================================
# Redis
# Using 6380 to avoid conflicts with local Redis installations
# =============================================================================
REDIS_PORT=6380
REDIS_PASSWORD=autoerp_redis

# =============================================================================
# Meilisearch
# =============================================================================
MEILISEARCH_PORT=7700
MEILISEARCH_KEY=autoerp_meili_master_key

# =============================================================================
# MinIO (S3-compatible storage)
# =============================================================================
MINIO_API_PORT=9000
MINIO_CONSOLE_PORT=9001
MINIO_ROOT_USER=autoerp_minio
MINIO_ROOT_PASSWORD=autoerp_minio_secret
